<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <!-- 设置背景色 -->
    <item android:id="@android:id/background">
        <shape>
          <!--   <corners android:radius="0dip" #00ffffff/> -->
            <gradient
                android:endColor="#8B8B8C"
                android:startColor="#8B8B8C" />
        </shape>
    </item>

    <!-- 设置进度条颜色 -->
    <item android:id="@android:id/progress">
        <clip>
            <shape>
               <!--  <corners android:radius="0dip" /> -->

                <gradient
                    android:endColor="#fe7f05"
                    android:startColor="#fe7f05" />
            </shape>
        </clip>
    </item>

</layer-list>